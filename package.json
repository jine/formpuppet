{
  "name": "formpuppet",
  "version": "1.6.1",
  "description": "A simple script in TypeScript, using Puppeteer to fill in a form and submit it on a fixed schedule.",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "start:prod": "node dist/index.js",
    "lint": "eslint index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "puppeteer": "^24.34.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.52.0"
  }
}
